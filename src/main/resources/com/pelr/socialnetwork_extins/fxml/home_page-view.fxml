<AnchorPane id="rootLayout" fx:id="rootLayout" prefHeight="700" prefWidth="1200" stylesheets="@../css/home_page_stylesheet.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pelr.socialnetwork_extins.controllers.HomePageController">

    

    <Button id="friendRequestsButton" fx:id="friendRequestsButton" layoutX="42.0" layoutY="291.0" mnemonicParsing="false" onAction="#onFriendRequestsButtonClicked" prefHeight="33.0" prefWidth="140.0" text="Friend Requests" />
    <Button id="profileButton" fx:id="profileButton" layoutX="42.0" layoutY="334.0" mnemonicParsing="false" onAction="#onProfileButtonClicked" prefHeight="33.0" prefWidth="140.0" text="My Profile" />
    <Button layoutX="42.0" layoutY="377.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="140.0" text="Button" />
